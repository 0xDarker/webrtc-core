{
  "name": "webrtc-media-core",
  "version": "1.0.0",
  "main": "./build/bundle.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": "git@sqbu-github.cisco.com:CPaaS/webrtc-media-core.git",
  "author": "arungane <arungane@cisco.com>",
  "license": "MIT",
  "devDependencies": {
    "@microsoft/api-extractor": "^7.18.4",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "eslint": "^7.29.0",
    "jsdoc": "^3.6.7",
    "mocha": "^9.0.2",
    "ts-loader": "^9.2.3",
    "typedoc": "^0.21.2",
    "typescript": "^4.3.5",
    "webpack": "^5.44.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  },
  "scripts": {
    "build": "npm run transpile && npm run compile && npm run docs",
    "clean": "npm run transpile:clean && npm run compile:clean && npm run docs:clean",
    "start": "node dist/app.js",
    "dev": "nodemon src/app.ts",
    "server": "webpack serve",
    "typedoc ": "typedoc --tsconfig typedoc.json",
    "docs": "npm run docs:clean && npm run docs:extract && npm run docs:generate",
    "docs:clean": "rimraf ./docs",
    "docs:extract": "api-extractor run -c ./api-extractor.json",
    "docs:generate": "api-documenter markdown -i ./docs/temp -o ./docs",
    "compile": "rollup -c ./rollup.config.js",
    "compile:clean": "rimraf ./build",
    "compile:watch": "rollup -c ./rollup.config.js -w",
    "transpile": "tsc",
    "transpile:clean": "rimraf ./dist",
    "transpile:validate": "tsc --noemit",
    "transpile:watch": "tsc --watch"
  },
  "dependencies": {
    "@microsoft/api-documenter": "^7.13.33",
    "@types/express": "^4.17.13",
    "@types/node": "^16.0.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsdoc": "^35.5.0",
    "express": "^4.17.1",
    "nodemon": "^2.0.9",
    "rollup": "^2.53.2",
    "rollup-plugin-terser": "^7.0.2",
    "webrtc-adapter": "^8.0.0"
  }
}
